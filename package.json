{
  "name": "winml-dashbaord",
  "version": "0.1.0",
  "description": "Dashboard for development in ONNX using Windows ML",
  "author": "Tiago Koji Castro Shibata <t-tica@microsoft.com>",
  "homepage": ".",
  "main": "src/electron.js",
  "private": true,
  "dependencies": {
    "@types/prop-types": "^15.5.3",
    "@types/react-redux": "^6.0.4",
    "ajv": "^6.5.2",
    "jsoneditor": "^5.18.0",
    "jsoneditor-react": "^0.1.9",
    "npm-font-open-sans": "^1.1.0",
    "office-ui-fabric-react": "^6.23.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-markdown": "^3.3.4",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "postinstall": "node scripts/postinstall.js && yarn copy-netron",
    "copy-netron": "yarn --cwd static/Netron && python copy_netron.py && echo 'TODO minify Netron bundle'",
    "electron-dev": "electron . http://localhost:3000",
    "electron-prod": "yarn build && electron ."
  },
  "build": {
    "appId": "com.microsoft.dashboard.winml",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "electron": "^2.0.4",
    "electron-packager": "^12.1.0",
    "typescript": "^2.9.2"
  }
}
